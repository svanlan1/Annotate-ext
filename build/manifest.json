{
  "name": "Annotate",
  "short_name": "Easily annotate any page on the web.",
  "version": "0.6.39",
  "manifest_version": 2,
  "author": "Shea VanLaningham",
  "options_page": "index.html",
  "description": "Quickly and easily add notes and annotations to any webpage and share with others.",
  "background": {
    "scripts": ["js/jquery.js", "js/background.js"],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/jscolor.js",
        "js/annotate.min.js", 
        "js/jquery.js", 
        "js/jquery-ui.js",
        "js/jquery.touchpunch.min.js"],
      "css": ["css/architects_daughter.css", "css/rocksalt.css", "css/annotate.css", "css/jquery-ui.structure.min.css"],
      "run_at" : "document_start"
    }
  ],
  "icons":{"16":"images/annotate_16_inactive.png", "128":"images/annotate_128.png"},
  "browser_action": {
    "default_icon": "images/annotate_16_inactive.png"
  },
  "permissions": [
    "tabs", "notifications", "<all_urls>", "http://*/*", "https://*/*", "contextMenus", "http://annotate.tech/login.php"
  ],
   "web_accessible_resources": [
